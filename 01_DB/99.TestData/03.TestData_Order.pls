-- データ削除
DELETE FROM ORDERS_DTL;
DELETE FROM ORDERS;

-- データ追加
BEGIN
  FOR i IN 1..10000
  LOOP
    INSERT INTO ORDERS(ORDER_ID,ORDER_USR_ID,ORDER_DATE,UPD_USER_ID,UPD_DATE)VALUES('ODR'|| i,'USR'|| i,CURRENT_DATE,'USR'|| i,CURRENT_DATE);
  END LOOP;

  INSERT INTO ORDERS(ORDER_ID,ORDER_USR_ID,ORDER_DATE,UPD_USER_ID,UPD_DATE)VALUES('ODR1A','USR1',CURRENT_DATE,'USR1',CURRENT_DATE);
  INSERT INTO ORDERS(ORDER_ID,ORDER_USR_ID,ORDER_DATE,UPD_USER_ID,UPD_DATE)VALUES('ODR2A','USR1',CURRENT_DATE,'USR1',CURRENT_DATE);

  FOR i IN 1..9996
  LOOP
    INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR'|| i,1,'GDS'|| (i+0),2,(i+0),'USR'|| i,CURRENT_DATE);
    INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR'|| i,2,'GDS'|| (i+1),2,(i+1),'USR'|| i,CURRENT_DATE);
    INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR'|| i,3,'GDS'|| (i+2),2,(i+2),'USR'|| i,CURRENT_DATE);
    INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR'|| i,4,'GDS'|| (i+3),2,(i+3),'USR'|| i,CURRENT_DATE);
    INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR'|| I,5,'GDS'|| (i+4),2,(i+4),'USR'|| I,CURRENT_DATE);
  END LOOP;

  INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR1A',1,'GDS99998',1,99998,'USR1',CURRENT_DATE);
  INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR1A',2,'GDS99999',1,99999,'USR1',CURRENT_DATE);
  INSERT INTO ORDERS_DTL (ORDER_ID,ORDER_DTL_NO,ORDER_GOODS_ID,ORDER_GOODS_NUM,ORDER_GOODS_UNIT_PRICE,UPD_USER_ID,UPD_DATE)VALUES('ODR2A',1,'GDS99999',2,99999,'USR1',CURRENT_DATE);

END;